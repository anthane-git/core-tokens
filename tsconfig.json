{
	"compilerOptions": {
		"target": "es2016",
		"module": "ES6",
		"moduleResolution": "node",
		"sourceMap": true,
		"baseUrl": "./src",
		"outDir": "./dist",
		"removeComments": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
